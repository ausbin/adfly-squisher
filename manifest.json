{
    "name" : "AdFly Squisher",
    "version" : "0.2.3",
    "description" : "Replace adfly links in pages with proxy links",
    "homepage_url" : "https://github.com/UncleNinja/adfly-squisher",
    "update_url" : "https://raw.github.com/UncleNinja/adfly-squisher/updater/updates.xml",

    "permissions" : [
        "tabs",
        "<all_urls>"
    ],
    "browser_action" : {
        "name" : "AdFly Squisher",
        "default_icon" : "icons/icon19.png",
        "default_title" : "Squish AdFlies on this page",
        "icons" : {
            "128" : "icons/icon128.png",
            "64"  : "icons/icon64.png",
            "48"  : "icons/icon48.png",
            "32"  : "icons/icon32.png",
            "19"  : "icons/icon19.png",
            "16"  : "icons/icon16.png",
            "8"   : "icons/icon8.png"
        }
    },
    "background_page" : "background.html",
    "options_page" : "options.html"
}
